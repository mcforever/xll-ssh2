<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.pojo.system">
	<class name="SysRole" table="sysrole" schema="laiyang" >
		<id name="id" column="id" type="java.lang.Integer">
			<generator class="sequence">
				<param name="sequence">seq_sysrole_id</param>
			</generator>
		</id>
		<property name="roleName" type="java.lang.String">
			<column name="roleName" length="100"></column>
		</property>
		<property name="menus" type="java.lang.String">
			<column name="MENUS" length="600"></column>
		</property>
		<property name="memo" type="java.lang.String">
			<column name="MEMO" length="600"></column>
		</property>
	</class>
</hibernate-mapping>
